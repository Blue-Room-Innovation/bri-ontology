    sh:maxCount 1 ;
    sh:node bri-shape:ProductShape ;
  ] ;

  sh:property [
    sh:path untp:granularityLevel ;
    sh:datatype xsd:string ;
    sh:in ( "item" "batch" "model" ) ;
    sh:maxCount 1 ;
  ] .

