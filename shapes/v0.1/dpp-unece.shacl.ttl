@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bri-shape:DigitalProductPassportShape
  a sh:NodeShape ;
  sh:targetClass untp:DigitalProductPassport ;
  sh:closed true ;

  sh:property [
    sh:path untp-core:id ;
    sh:datatype xsd:anyURI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

  sh:property [
    sh:path vc:issuer ;
    sh:class untp:CredentialIssuer ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:node bri-shape:CredentialIssuerShape ;
  ] ;

  # validFrom / validUntil
  sh:property [
    sh:path untp-core:validFrom ;
    sh:datatype schema:DateTime ;
    sh:maxCount 1 ;
  ] ;

  sh:property [
    sh:path untp-core:validUntil ;
    sh:datatype schema:dateTime ;
    sh:maxCount 1 ;
  ] ;

  sh:property [
    sh:path vc:credentialSubject ;
    sh:class untp:ProductPassport ;
    sh:node bri-shape:ProductPassportShape ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] .

bri-shape:CredentialIssuerShape
  a sh:NodeShape ;
  sh:targetClass untp-core:CredentialIssuer ;
  sh:closed false ;

  sh:property [
    sh:path untp-core:id ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

  sh:property [
    sh:path untp-core:issuerAlsoKnownAs ;
    sh:minCount 0 ;
    sh:maxCount 1 ;
    sh:node bri-shape:PartyShape ;
  ] ;

  sh:property [
    sh:path untp-core:name ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] .


