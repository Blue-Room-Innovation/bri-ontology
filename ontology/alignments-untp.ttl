@prefix dpp:     <https://example.org/dpp#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix untpdpp: <https://test.uncefact.org/vocabulary/untp/dpp/0/> .
@prefix untpcore:<https://test.uncefact.org/vocabulary/untp/core/0/> .

# Alineaciones DPP con UNTP (UN/CEFACT)

dpp:DigitalProductPassport owl:equivalentClass untpdpp:DigitalProductPassport ;
  rdfs:seeAlso untpdpp:ProductPassport .

dpp:Product owl:equivalentClass untpcore:Product .

dpp:Organization rdfs:subClassOf untpcore:Party .

dpp:Person rdfs:subClassOf untpcore:Party .

dpp:Facility a owl:Class ;
  rdfs:label "Instalación"@es ;
  rdfs:comment "Instalación o planta donde se realizan procesos."@es ;
  rdfs:subClassOf dpp:Location ;
  owl:equivalentClass untpcore:Facility .

dpp:Certificate rdfs:seeAlso untpcore:Declaration , untpcore:Claim .

dpp:Identifier rdfs:seeAlso untpcore:IdentifierScheme .

dpp:LifecycleEvent rdfs:seeAlso untpcore:traceabilityEvent .

dpp:Measurement rdfs:seeAlso untpcore:value , untpcore:unit .

dpp:describesProduct owl:equivalentProperty untpcore:product .

dpp:manufacturer rdfs:seeAlso untpcore:producedByParty .

dpp:hasCertificate rdfs:seeAlso untpcore:conformityEvidence , untpcore:dueDiligenceDeclaration .

dpp:serialNumber owl:equivalentProperty untpcore:serialNumber .

dpp:lotNumber owl:equivalentProperty untpcore:batchNumber .
