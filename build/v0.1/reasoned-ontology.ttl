###  https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/digitalMarpolWastePassport.ttl#contactPoint
:contactPoint rdf:type owl:ObjectProperty ;
              rdfs:domain :AuthorizedParty ;
              rdfs:range <https://ontology.untp.io/core/Link> ;
              rdfs:comment "Contact information for the authorized party, represented as a link." .


###  https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/digitalMarpolWastePassport.ttl#credentialSubject
:credentialSubject rdf:type owl:ObjectProperty ;
                   rdfs:domain :DigitalMarpolWastePassport ;
                   rdfs:range :MarpolWastePassport ;
                   rdfs:comment "References the MARPOL-specific waste passport subject (`MarpolWastePassport`)." .


###  https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/digitalMarpolWastePassport.ttl#estimatedGenerated
:estimatedGenerated rdf:type owl:ObjectProperty ;
                    rdfs:domain :ResidueInformation ;
                    rdfs:range <https://ontology.untp.io/core/Measure> ;
                    rdfs:comment "The estimated quantity of residue generated, represented as a measure." .


###  https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/digitalMarpolWastePassport.ttl#involvedParty
:involvedParty rdf:type owl:ObjectProperty ;
               rdfs:domain :MarpolWaste ;
               rdfs:range :InvolvedParty ;
               rdfs:comment "The party involved in the waste management process, such as the ship operator or company." .


