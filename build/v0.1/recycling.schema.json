{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "$defs": {
    "bri_recycling_RecyclingOrganisationShape": {
      "allOf": [
        {
          "type": "object",
          "properties": {
            "managerCode": {
              "type": "string",
              "minLength": 1
            },
            "nimaCode": {
              "type": "string",
              "minLength": 1
            },
            "name": {
              "type": "string",
              "minLength": 1
            },
            "streetAddress": {
              "type": "string",
              "minLength": 1
            },
            "addressLocality": {
              "type": "string",
              "minLength": 1
            },
            "postalCode": {
              "type": "string",
              "minLength": 1
            },
            "correspondeceAddress": {
              "type": "string",
              "minLength": 1
            },
            "correspondeceAddressLocality": {
              "type": "string",
              "minLength": 1
            },
            "correspondecePostalCode": {
              "type": "string",
              "minLength": 1
            },
            "telephone": {
              "type": "string",
              "pattern": "^[0-9 +()]{6,}$"
            },
            "faxNumber": {
              "type": "string",
              "pattern": "^[0-9 +()]{6,}$"
            },
            "email": {
              "type": "string",
              "pattern": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$"
            },
            "url": {
              "type": "string",
              "pattern": "^(?!_:).+:.+"
            },
            "latitude": {
              "anyOf": [
                {},
                {
                  "type": "array",
                  "items": {},
                  "minItems": 0
                }
              ]
            },
            "longitude": {
              "anyOf": [
                {},
                {
                  "type": "array",
                  "items": {},
                  "minItems": 0
                }
              ]
            },
            "adaptedToRD1102015": {
              "type": "boolean",
              "enum": [
                true,
                false
              ]
            },
            "wasteTreatmentActivity": {
              "type": "string"
            },
            "@type": {
              "enum": [
                "Recycler",
                "https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/recycling.ttl#Recycler"
              ]
            },
            "type": {
              "enum": [
                "Recycler",
                "https://blue-room-innovation.github.io/bri-ontology/ontology/v0.1/recycling.ttl#Recycler"
              ]
            },
            "@id": {
              "type": "string"
            },
            "id": {
              "type": "string"
            }
          },
          "required": [
            "@type",
            "addressLocality",
            "correspondeceAddress",
            "correspondeceAddressLocality",
            "correspondecePostalCode",
            "managerCode",
            "name",
            "nimaCode",
            "postalCode",
            "streetAddress"
          ],
          "unevaluatedProperties": false,
          "additionalProperties": false
        },
        {
          "anyOf": [
            {
              "allOf": [
                {
                  "type": "object",
                  "properties": {}
                },
                {
                  "not": {
                    "type": "object",
                    "properties": {
                      "latitude": {
                        "anyOf": [
                          {},
                          {
                            "type": "array",
                            "items": {},
                            "minItems": 1
                          }
                        ]
                      }
                    },
                    "required": [
                      "latitude"
                    ]
                  }
                },
                {
                  "not": {
                    "type": "object",
                    "properties": {
                      "longitude": {
                        "anyOf": [
                          {},
                          {
                            "type": "array",
                            "items": {},
                            "minItems": 1
                          }
                        ]
                      }
                    },
                    "required": [
                      "longitude"
                    ]
                  }
                }
              ]
            },
            {
              "type": "object",
              "properties": {
                "latitude": {
                  "type": "number",
                  "minimum": -90,
                  "maximum": 90
                },
                "longitude": {
                  "type": "number",
                  "minimum": -180,
                  "maximum": 180
                }
              },
              "required": [
                "latitude",
                "longitude"
              ]
            }
          ]
        }
      ],
      "unevaluatedProperties": false
    }
  },
  "anyOf": [
    {
      "type": "object",
      "properties": {
        "@graph": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/bri_recycling_RecyclingOrganisationShape"
              }
            ]
          }
        }
      },
      "required": [
        "@graph"
      ]
    },
    {
      "$ref": "#/$defs/bri_recycling_RecyclingOrganisationShape"
    }
  ]
}